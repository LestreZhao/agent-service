<script>
    $(window).keydown(function(event) {  //键盘敲击事件
        if (event.keyCode == 13) {
            var flag = $(".maskbg").is(":visible");
            if(flag){
                login2();
            }
        }
    });
    function login2() {
        var userName = $("#username").val();
        if(userName == ''){
            layer.msg("请输入用户名。");
            return;
        }
        var userPass = $("#password").val();
        if(userPass == ''){
            layer.msg("请输入密码。");
            return;
        }
        var messageCode = $("#message_code").val();
        var messageCodeText=$.trim(messageCode); //
        if(messageCodeText.length<1){
            layer.msg("验证码不能为空！");
            return null;
        }
        var encrypt = new JSEncrypt();
        var encrypted_name = encrypt.encrypt(userName);
        var encrypted_userPass = encrypt.encrypt(userPass);
        var flag = $("#kaptchaShow").is(':hidden');
        if(!flag){
            var kaptcha = $.trim($("#kaptcha").val());
            if(kaptcha == ''){
                layer.msg("请输入验证码。");
                return;
            }
        }
        $.ajax({
            type : "post",
            url : "/userlogin.do",
            data : {
                "username" : encrypted_name,
                "password" : encrypted_userPass,
                "kaptcha"  : kaptcha,
                "code" : messageCodeText
            },
            dataType : "json",
            async : false,
            success : function(res) {
                data = res.data;
                layer.closeAll('loading');
                if ("ok"==data.tishi) {
                    $(".unlogin").hide();
                    $(".login").show();
                    window.location.href = "/index.html";
                }else if("hezuo" == data.tishi){
                    layer.msg("该机构已升级为合作机构，请后台登录！");
                    $(".prohibitBox2").find("span").html("该机构已升级为合作机构，请后台登录");
                    $(".prohibitBox2").show();
                } else if ("nouser" == data.tishi) {
                    layer.msg("用户名不存在！");
                    $(".prohibitBox2").find("span").html("用户名不存在");
                    $(".prohibitBox2").show();
                } else if ("weijihuo" == data.tishi) {
                    layer.msg("该用户未激活，请去注册邮箱激活！");
                    $(".prohibitBox2").find("span").html("该用户未激活，请去注册邮箱激活！<br><a onclick='jumpEmail(\""+data.email+"\")' style='color: #0381bb;'>去邮箱</a>").css("font-size","16px");
                    $(".prohibitBox2").show();
                } else if ("locking" == data.tishi) {
                    layer.msg("该用户已被锁定，请联系管理员！",{time:300000});
                    $(".prohibitBox2").find("span").html("该用户已被锁定，请联系管理员");
                    $(".prohibitBox2").show();
                } else if ("no"==data.tishi||"coreerro" == data.tishi) {
                    if("no"==data.tishi){
                        layer.msg("请检查账号、密码是否正确！");
                    }else{
                        layer.msg("手机或邮箱验证码错误！");
                    }
                    $("#kaptcha").val("");
                    if(data.kaptchaTime <3) {
                        $("#kaptchaShow").hide();
                        $(".prohibitBox1").hide();
                    }else if(data.kaptchaTime >=3 && data.kaptchaTime <6){
                        $("#kaptchaShow").show();
                        $("#count").html(6 - (data.kaptchaTime < 6 ? data.kaptchaTime : 6));
                        $(".prohibitBox1").show();
                        changeCode();
                    }else{
                        $(".prohibitBox1").hide();//连续输错6次密码锁定账号
                        $(".prohibitBox2").find("span").html("该用户已被锁定，请联系管理员");
                        $(".prohibitBox2").show();
                        changeCode();
                    }
                } else if ("kaptcha"==data.tishi) {
                    layer.msg("验证码有误！请重新输入！");
                    $("#kaptchaShow").show();
                    $("#kaptcha").val("");
                    changeCode();
                } else if ("refuse" == data.tishi) {
                    if(data.type == '1'){//个人
                        layer.msg("该账号已被禁用，请联系管理员！");
                    }else{
                        layer.msg("该账号已被拒绝，请联系管理员！");
                    }
                } else if ("untreated" == data.tishi) {
                    layer.msg("该账号等待处理，请联系管理员！");
                }else if ("kong" == data.tishi) {
                    layer.msg("登陆信息为空！");
                }else if ("recovery" == data.tishi) {
                    layer.msg("账号密码已过有效期，请您先修改密码再登录！",{time:2000},function(){
                        window.location.href = "/user/recovery_password.html";
                    });
                }
            }
        });


    }
    function sendAuthenticationCodess(id){
        var userName = $("#username").val();
        if(userName == ''){
            layer.msg("请输入用户名。");
            return;
        }
        var userPass = $("#password").val();
        if(userPass == ''){
            layer.msg("请输入密码。");
            return;
        }

        var encrypt = new JSEncrypt();
        var encrypted_name = encrypt.encrypt(userName);
        var encrypted_userPass = encrypt.encrypt(userPass);
        $.ajax({
            type : "post",
            url : "/user/sendMessagerLogin.do",
            data : {
                "username" : encrypted_name,
                "password": encrypted_userPass

            },
            dataType : "json",
            async : false,
            success : function(data) {
                countdowns = 60;
                circulateCode2s(id);
                if (data.status==200) {
                    layer.msg(data.data);
                }else if(data.status==305){
                    layer.msg("账号密码已过有效期，请您先修改密码再登录！",{time:2000},function(){
                        window.location.href = "/user/recovery_password.html";
                    });
                }else{
                    layer.msg(data.msg);
                }
            },
            error : function(data) {
                layer.msg("请求失败");
            }
        });
    }
    var countdowns=0;
    function circulateCode2s(id) {
        if (countdowns == 0) {
            $("#" + id).html("获取验证码");
            $("#" + id).attr("onclick", "sendAuthenticationCodess(" + id + ")");
        } else {
            $("#" + id).html("重新发送(" + countdowns + ")");
            $("#" + id).removeAttr("onclick");
            countdowns--;
            setTimeout(function () {
                circulateCode2s(id)
            }, 1000);
        }
    }
    function longchangeCode() {  //刷新
        $('#kaptchaImage').hide().attr('src', '/kaptcha.jpg?' + Math.floor(Math.random()*100) ).fadeIn();
    }
