        /**
         * 用于下载导入模板时的影藏form表单的提交，采用post方式提交
         * @param action action映射地址
         * @param type parameter的名称
         * @param value parameter的值，这里为file的filename
         */
        function downloadTemplate(action, type, value) {
            var form = document.createElement('form');
            document.body.appendChild(form);
            form.style.display = "none";
            form.action = action;
            form.id = 'excel';
            form.method = 'post';

            var newElement = document.createElement("input");
            newElement.setAttribute("type", "hidden");
            newElement.name = type;
            newElement.value = value;
            form.appendChild(newElement);
            form.submit();
        }

        changeCode();
    });

    function isLogin() {
        $.ajax({
            url: '/user/userIsLogin.do',
            type: 'post',
            data: {},
            dataType: 'json',
            success: function (result) {
                if (result.success) {
                    window.location.href = '/userData/user_data.html?data_type=data_subscription';
                } else {
                    $(".maskbg").show();$('#kaptchaImage').hide().attr('src', '/kaptcha.jpg?' + Math.floor(Math.random()*100) ).fadeIn();
                }
            }
        });
    }

    function changeCode() {  //刷新
        $('#kaptchaImage').hide().attr('src', '/kaptcha.jpg?' + Math.floor(Math.random() * 100)).fadeIn();
    }
</script>
