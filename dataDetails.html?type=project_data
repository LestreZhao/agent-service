<script type="text/javascript">

    var myDate = new Date;
    var year = myDate.getFullYear();
    var mon = myDate.getMonth() + 1;
    var date = myDate.getDate();
    var now = year+'.'+getNow(mon)+"."+getNow(date);

    //判断是否在前面加0
    function getNow(s) {
        return s < 10 ? '0' + s: s;
    }

	$('.records_date_info').text(now);


    var _paq = _paq || [];
    /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
    _paq.push(['trackPageView']);
    _paq.push(['enableLinkTracking']);
    (function() {
        var u="//analytics.escience.org.cn/";
        _paq.push(['setTrackerUrl', u+'piwik.php']);
        _paq.push(['setSiteId', '2']);
        var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
        g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'piwik.js'; s.parentNode.insertBefore(g,s);
    })();
    $(function() {
		getnumanalytics();
		$.ajax({
			type: 'post',
			url: '/index/analytics.do',
			data: {'url': "setTrackerUrl"},
			dataType: 'json',
			success: function (data) {
				// layer.msg(data);
			}
		});


    });
	function getnumanalytics() {
		$.ajax({
			type: 'post',
			url: '/index/getnumanalytics.do',
			data: {},
			dataType: 'json',
			success: function (data) {
				if(data.status == 200){
					$('.visitor_volume').html(data.data);
				}
			}
		});
	}

	function escapemy(str) {
		return str;
	}
	function escthingf() {
		$.ajax({
			type: "post",
			url: "/loginLayout.do",
			data: {},
			dataType: "json",
			async: false,
			success: function (data) {
				deleteCookie("userName", "/");
				deleteCookie("userPass", "/");
			}
		});
		window.location.href = "/phda/login.html";
	}


	function escthing() {

		layui.use(['layer'], function () {
			var layer = layui.layer;
			layer.open({
				type: 1,
				move: false,
				area: 'auto',
				resize: false,
				offset: 'auto',
				maxWidth: 'auto',
				maxHeight: 'auto',
				id: 'layerDemoauto' + "auto",
				title: [
					'',
					'background: none; border-bottom: 0;'
				],
				content: $("#exit_dialog_content"),
				btn: ['取消', '确定'],
				success: function (layer, index) {
					$(layer[0]).addClass('exit_dialog_box')
				}
				,btn2: function(index, layero){
					escthingf();
				}
			});
		});
	}
</script>
